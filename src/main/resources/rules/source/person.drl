package rules.hello
import com.sheliming.drools.Person

rule "ageLower18"
    when
        $person: Person(age < 18)
    then
        System.out.println($person.getName()+"未成年");
    end

rule "ageUper18"
    when
        $person: Person(age >= 18)
    then
        System.out.println($person.getName()+"已成年");
    end